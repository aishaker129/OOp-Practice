#include <algorithm>
#include<iostream>
#include<string>
using namespace std;
int main()
{
   int n,h,sum=0;
   cin>>n>>h;
   for(int i=0;i<n;i++){
        int t;
      cin>>t;
    if(t<=h)
    {
        sum=sum+1;
    }
    else
    {
        sum=sum+2;
    }
   }
   cout<<sum<<endl;
    return 0;
}
